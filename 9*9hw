#include <stdio.h>

int main() {
    for (int i = 1; i <= 9; i += 3) {
        for (int j = 1; j <= 9; j++) {
            printf("%d X %d = %2d     ", i, j, i*j);
            printf("%d X %d = %2d     ", i+1, j, (i+1)*j);
            printf("%d X %d = %2d\n", i+2, j, (i+2)*j);
        }
        printf("\n\n");
    }
    return 0;
}
